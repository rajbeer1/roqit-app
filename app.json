{
  "expo": {
    "name": "Roqit",
    "slug": "roqit-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Roqit uses the camera to capture profile, document, and trip photos for verification and proof of service.",
        "NSPhotoLibraryUsageDescription": "Roqit needs access to your photo library to let you select existing images for profile, document uploads, and payment proofs.",
        "NSLocationWhenInUseUsageDescription": "Roqit uses your location while the app is in use to show nearby hubs, improve route accuracy, and attach location context to trips.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.rajbeer11.roqitapp",
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQmnLW023firNhFBLdQVyhTZeqXIIYEuE"
        }
      },
      "usesCleartextTraffic": false,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "jsEngine": "hermes",
      "package": "com.rajbeer11.roqitapp",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Roqit to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Roqit uses your location while the app is in use to show nearby hubs, improve route accuracy, and attach location context to trips."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0804ff3f-c80d-4455-a0d4-84fb0d77ebcd"
      }
    },
    "owner": "rajbeer11"
  }
}
