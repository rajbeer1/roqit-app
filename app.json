{
  "expo": {
    "name": "roqit-app",
    "slug": "roqit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to take photos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.rajbeer11.roqitapp",
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQmnLW023firNhFBLdQVyhTZeqXIIYEuE"
        }
      },
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "jsEngine": "hermes",
      "package": "com.rajbeer11.roqitapp",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_WIFI_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "react-native-gesture-handler",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Roqit to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Roqit to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0804ff3f-c80d-4455-a0d4-84fb0d77ebcd"
      }
    },
    "owner": "rajbeer11"
  }
}
