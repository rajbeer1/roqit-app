{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_IDENTITY_URL": "https://identity.app.roqit.com",
        "EXPO_PUBLIC_BACKEND_URL": "https://api.app.roqit.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
